spring:
  application:
    name: {SERVICE-NAME}
  jpa:
    hibernate:
      ddl-auto: update
  datasource:
    url: {JDBC}
    username: {USER}
    password: {PASSWORD}
eureka:
  client:
    registerWithEureka: true
    fetchRegistry: true
    serviceUrl:
      defaultZone: http://localhost:8761/eureka
